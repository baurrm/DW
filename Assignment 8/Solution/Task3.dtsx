      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{7495B1B6-2FAD-4B3D-909D-BAFB6F5206E0}"
          SQLTask:SqlStatementSource="DECLARE @BeginDate DATETIME&#xA;DECLARE @EndDate DATETIME&#xA;&#xA;SET @BeginDate = (SELECT MIN(OrderDate) FROM AWsrc.SalesOrderHeader);&#xA;SET @EndDate = (SELECT MAX(DueDate) FROM AWsrc.SalesOrderHeader);&#xA;&#xA;IF @BeginDate &gt;  (SELECT MIN(ShipDate) FROM AWsrc.SalesOrderHeader)&#xA;&#x9;SET @BeginDate =  (SELECT MIN(ShipDate) FROM AWsrc.SalesOrderHeader)&#xA;&#xA;IF @BeginDate &gt;  (SELECT MIN(DueDate) FROM AWsrc.SalesOrderHeader)&#xA;&#x9;SET @BeginDate =  (SELECT MIN(Duedate) FROM AWsrc.SalesOrderHeader)&#x9;&#xA;&#xA;IF @EndDate &lt;  (SELECT MAX(ShipDate) FROM AWsrc.SalesOrderHeader)&#xA;&#x9;SET @EndDate =  (SELECT MAX(ShipDate) FROM AWsrc.SalesOrderHeader)&#xA;&#xA;IF @EndDate &lt;  (SELECT MAX(OrderDate) FROM AWsrc.SalesOrderHeader)&#xA;&#x9;SET @EndDate =  (SELECT MAX(OrderDate) FROM AWsrc.SalesOrderHeader)&#xA;&#xA;DECLARE @TempTimeTable Table(&#xA;&#x9;FullDate DATE&#xA;);&#xA;&#xA;WITH TempTimeTable AS (&#xA;&#x9;SELECT CAST(@BeginDate AS DATETIME) FullDate&#xA;&#x9;UNION ALL &#xA;&#x9;SELECT DATEADD(dd, 1, FullDate)&#xA;&#x9;FROM TempTimeTable&#xA;&#x9;WHERE DATEADD(dd, 1, FullDate) &lt;= CAST(@EndDate AS DATETIME)&#xA;)&#xA;SELECT FullDate&#xA;INTO AWstage.StageTime&#xA;FROM TempTimeTable&#xA;OPTION (MAXRECURSION 2000); " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
